<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 사각형의 색상 설정 -->
    <solid android:color="#66A3B18A" /> <!-- 원하는 색상으로 변경 가능 -->

    <!-- 사각형의 모서리를 둥글게 설정 -->
    <corners
        android:radius="10dp" /> <!-- 둥근 정도 설정 -->

    <!-- 사각형의 테두리 설정 -->
    <stroke
        android:width="3dp"
        android:color="#D9D9D9" /> <!-- 테두리 색상 설정 -->

</shape>